{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "background": {
    "permissions": ["activeTab"]
  },
  "content_scripts": [
    { 
      "matches" : ["https://*/*"],
      "js": ["background.js"],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}